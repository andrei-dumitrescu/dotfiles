printHelp() {
  echo "NAME"
  echo "  $scriptName"
  echo ""
  echo "SYNOPSIS"
  echo "  $scriptName [options]... [title] [message]"
  echo ""
  echo "DESCRIPTION"
  echo "  Doomguy themed dunst notification" 
  echo ""
  echo "OPTIONS"
  echo "  -h, --help                        Print this help and exit"
  echo "  -t, --title                       Title of the notification"
  echo "  -m, --message                     Message of the notification"
  echo "  -p, --type [=info|error|success]  Type of the notification"
}
