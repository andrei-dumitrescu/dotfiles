##
##
##

# source $HOME/.local/bin/zsh-plugins/fzf-tab/fzf-tab.plugin.zsh

##
## Syntax highlighting
##

source $HOME/.local/bin/zsh-plugins/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh

##
## Fish like history search feature when pressing UP or DOWN
##

source $HOME/.local/bin/zsh-plugins/zsh-history-substring-search/zsh-history-substring-search.zsh

bindkey '^[[A' history-substring-search-up
bindkey '^[[B' history-substring-search-down

##
## Fish like autosuggestions
##

source $HOME/.local/bin/zsh-plugins/zsh-autosuggestions/zsh-autosuggestions.zsh

##
## Node Version Manager
##

source $HOME/.local/bin/zsh-plugins/zsh-nvm/zsh-nvm.plugin.zsh

##
## Powerlevel10k prompt 
##

source $HOME/.local/bin/zsh-plugins/powerlevel10k/powerlevel10k.zsh-theme

# To customize prompt, run `p10k configure` or edit ~/.config/zsh/.zsh-p10k
[[ ! -f $HOME/.config/zsh/.zsh-p10k ]] || source $HOME/.config/zsh/.zsh-p10k
